{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/test.py"
    },
    {
      "src": "/api/jobs",
      "dest": "api/jobs.py"
    },
    {
      "src": "/api/generate-application",
      "dest": "api/generate-application.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
